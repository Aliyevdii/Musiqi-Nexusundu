{
  "name": "Music Bot",
  "description": "Telegram'ınızın Grup Sesli Sohbeti'nde şarkı çalmak için Açık Kaynaklı bot. Powered by Mehmetbaba55.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/BirBeyfendi/EfsaneMusicVaves-1",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram oturum dizesi",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bir bot belirteci @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "MusicPlayer Bot Adınız.",
      "required": true,
      "value": ""
    },
    "BOT_USERNAME": {
      "description": "MusicPlayer Bot Kullanıcı Adınız.",
      "required": true,
      "value": ""
    },
    "ASSISTAN_USERNAME": {
      "description": "Asistan Hesabınızın Kullanıcı Adını Yazınız.",
      "required": true,
      "value": ""
    },
    "HEROKU_API_KEY": {
      "description": "Heroku api anahtarınız (Ayarlar veya çark simgesine basarak alın) https://dashboard.heroku.com/account",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku uygulamanızın adı",
      "required": true
    },
    "API_ID": {
      "description": "my.telegram'dan uygulama kimliği.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "My.telegram'dan uygulama apı-hash.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Her yerde yönetici olarak sayılan kullanıcı kimliklerinin listesi (Müzik hesabınının id'si).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "İndirmeler için en fazla ses süresi sınırı (minutes).",
      "required": true,
      "value": "55"
    }
  }
